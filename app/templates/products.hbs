  <div class="mt-8 grid grid-cols-1 gap-x-6 gap-y-10 sm:grid-cols-2 lg:grid-cols-3 xl:grid-cols-3 xl:gap-x-8">
{{#if this.model}}
{{#each this.model as |product|}}
<Product @product={{product}} />
{{/each}}
    {{else}}
  loading

{{/if}}
  </div>

   </tab.pane>
   <tab.pane @title="Wine">
